{
  "name": "orchestrator-ordermanagement",
  "version": "1.0.0",
  "description": "orchestrator order management",
  "author": "Hansin Susatya",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "NODE_ENV=development nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node -r module-alias/register dist/main.js",
    "start:pm2": "pm2 start npm --name 'orchestrator' -- run start:prod",
    "restart:pm2": "pm2 stop orchestrator && pm2 delete orchestrator && pm2 start npm --name 'orchestrator' -- run start:prod",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "_moduleAliases": {
    "@services": "dist/services",
    "@interfaces": "dist/interfaces",
    "@controllers": "dist/controllers",
    "@producers": "dist/producers",
    "@helpers": "dist/helpers"
  },
  "dependencies": {
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^7.0.7",
    "@nestjs/core": "^7.0.7",
    "@nestjs/microservices": "^7.0.7",
    "@nestjs/platform-express": "^7.0.7",
    "@types/dotenv": "^8.2.0",
    "amqp-connection-manager": "^3.2.0",
    "amqplib": "^0.5.6",
    "bull": "^4.1.1",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "grpc": "^1.24.2",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "nats": "^1.4.12",
    "nest-router": "^1.0.9",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "typescript": "^3.9.10"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.7",
    "@types/bull": "^3.15.7",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^14.18.10",
    "@types/supertest": "^2.0.5",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
